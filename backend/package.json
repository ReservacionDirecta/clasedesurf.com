{
  "name": "clasedesurf-backend",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "start": "node scripts/start.js",
    "start:dev": "ts-node src/server.ts",
    "start:prod": "npx prisma migrate deploy && node dist/server.js",
    "start:simple": "node dist/server.js",
    "build": "npx prisma generate && tsc -p .",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init --preview-feature",
    "prisma:deploy": "prisma migrate deploy",
    "seed": "ts-node prisma/seed.ts",
    "seed:beaches": "ts-node prisma/seed_beaches.ts",
    "test": "npx vitest run",
    "typecheck": "tsc --noEmit",
    "lint": "echo 'lint step skipped'",
    "prepare": "husky || true"
  },
  "prisma": {
    "seed": "npm run seed"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@types/bcrypt": "^6.0.0",
    "@types/ioredis": "^4.28.10",
    "@types/jsonwebtoken": "^9.0.10",
    "@wppconnect-team/wppconnect": "^1.37.5",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cloudinary": "^2.8.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-rate-limit": "^8.1.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "pg": "^8.16.3",
    "resend": "^6.6.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.17",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.4.2",
    "prisma": "^5.22.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0",
    "husky": "^9.1.7",
    "vitest": "^0.34.0"
  }
}
